{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/\\w+/.*", "dest": "index.js" },
      { "src": "/\\w+/favicon\\.ico", "dest": "index.js" },
      {
        "src": "/static-page",
        "dest": "index.js"
      },{"src": "/.*", "dest":"index.js"}

    ]
  }
